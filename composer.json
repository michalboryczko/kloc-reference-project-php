{
    "name": "kloc/reference-project-php",
    "description": "Reference PHP project for testing scip-php indexer with Symfony patterns",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.4",
        "symfony/framework-bundle": "^7.2",
        "symfony/messenger": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/property-access": "^7.2",
        "symfony/validator": "^7.2",
        "symfony/runtime": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {},
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    }
}
