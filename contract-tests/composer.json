{
    "name": "kloc/contract-tests",
    "description": "Contract testing framework for validating scip-php calls.json output",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "ContractTests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContractTests\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
